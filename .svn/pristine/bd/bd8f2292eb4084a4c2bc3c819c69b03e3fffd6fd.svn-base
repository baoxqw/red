/* page/component/index.wxss */

.squre {
  width: 300rpx;
  height: 300rpx;
  /* width:40%; *//* height:calc(100% / 4 ); */
  float: left;
  text-align: center;
  box-sizing: border-box;
  display: flex;
  margin: 10rpx 2% 30rpx 5%;
  border-radius: 12rpx;
  justify-content: center;
  flex-direction: column;
  position: relative;
  overflow: hidden;
}

.squre view {
  margin-top: 20px;
  color: #fff;
  font-size: 30rpx;
}

.index-img {
  display: block;
  margin: 0 auto;
  width: 50rpx;
  height: 50rpx;
}

/* //地块 */

.landimg {
  width: 100%;
  height: 100%;
}

.content {
  position: absolute;
  top: 20%;
  width: 100%;
}

.con {
  width: 95%;
  margin-left: 16rpx;
  box-sizing: border-box;
  padding-top: 20rpx;
}
.backed{

  width:80%;
  height:70rpx;
  line-height: 70rpx;
  text-align: center;
  background: rgb(25,139,254);
  color:#fff;
  font-size: 40rpx;
  margin:20rpx auto;
  border-radius: 28rpx;
}
