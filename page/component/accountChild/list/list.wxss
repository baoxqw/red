.list{
  width:92%;
  left:4%;
  background: #fff;
  font-size: 30rpx;
  box-sizing: border-box;
  padding:0% 24rpx;
  margin-bottom:20rpx;
  border-radius: 10rpx;
  position: relative;
  overflow:hidden;
}
.first{
  margin-top:20rpx;
}
.list-tit{
  font-size: 36rpx;
  height:70rpx;
  line-height: 70rpx;
  margin-top:15rpx;
}
.list-con{
  height:60rpx;
  line-height:60rpx;
  color:rgb(132,132,132)
}
.list-con>view{
  display: inline-block;
  color:rgb(100,100,100);
}

.list-infor{
 float: left;
 width:80%;
 box-sizing: border-box;
 padding-bottom:10rpx;
  
}
.list-btn{
  float: right;
  width:19%;
}
.list-add{
  position: absolute;
  width:100%;
  font-size: 24rpx;
  top:20rpx;
  color:rgb(26,139,255);
}
.list-up{
  position: absolute;
  font-size: 22rpx;
  right:100rpx;
  bottom:22rpx;
  color:rgb(255,130,54);
  top:10%!important;
}
.list-del{
  position: absolute;
  font-size: 22rpx;
  bottom:22rpx;
  right:24rpx;
  color:rgb(255,54,54);
}
/* 遮罩层 删除*/
.del-wrap_hide{
  width:100%;
  height:100%;
  background:rgba(0, 0, 0, 0.3);
  position:fixed;
  display: none;
  left:0;
  top:0;
}
.del-wrap_show{
  width:100%;
  height:100%;
  background:rgba(0, 0, 0, 0.15);
  position:fixed;
  display: block;
  left:0;
  top:0;
}
.del-wrap_hide>view{
  margin:50% auto auto auto;
  width:560rpx;
  height:242rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
