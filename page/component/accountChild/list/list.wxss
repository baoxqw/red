/* 遮罩层  添加轮次 */
.round_show{
  width:100%;
  height:100%;
  background:rgba(0, 0, 0, 0.4);
  position:fixed;
  display: block;
  font-size: 30rpx;
}
.round_hide{
  width:100%;
  height:100%;
  background:rgba(0, 0, 0, 0.4);
  position:fixed;
  display: none;
  font-size: 30rpx;
}
.addround{
  margin:50% auto auto auto;
  width:560rpx;
  height:272rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.roundTitle{
  text-align: center;
  font-size: 30rpx;
}
.roundMain{
  height:70rpx;
  margin:40rpx 0;

}
.roundMain view{
  float: left;
  margin-left:50rpx;
  margin-top:10rpx;
  margin: 10rpx 50rpx 0 50rpx;
}
.roundMain input{
  float: left;
  width:250rpx;
  border:1px solid #f2f2f2;
}
.roundBtn{
  position: relative;
}
.roundNo{
  position: absolute;
  left:20%;
  width:140rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  border:1px solid #f2f2f2;
  border-radius: 30rpx;
}
.roundYes{
  position: absolute;
  right:20%;
  width:140rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  border:1px solid #f2f2f2;
  border-radius: 30rpx;
  color:#fff;
  background: rgb(25,139,254);
}
.bottom-line{
  width:100%;
  font-size:30rpx;
  height:40rpx;
  line-height:40rpx;
  text-align:center;
}